/* pages/logs/index.wxss */
/* 首页轮播图样式 */
.lunbo{
  height: 340rpx;
}
.lunbo navigator{
  width: 100%;
  height: 100%;
}
/* 首页导航栏样式 */
.nav{
  display: flex;
  justify-content: space-between;
  padding: 0 30rpx;
  margin-top: 12rpx;
  margin-bottom: 43rpx;
}
.nav navigator{
  width: 128rpx;
  height: 140rpx;
}
/* 首页楼层样式 */
.floor .floor-title{
  height: 59rpx;
}
.floor .floor-imgs{
  overflow: hidden;
  padding: 10rpx 16rpx;
}
.floor .floor-imgs navigator{
  float: left;
  height: 188rpx;
  margin-left: 15rpx;
}
.floor .floor-imgs navigator:nth-child(1){
  width: 232rpx;
  height: 386rpx;
  margin-left: 0;
}
.floor .floor-imgs navigator:nth-child(2),.floor .floor-imgs navigator:nth-child(4){
  width: 233rpx;
  margin-bottom: 10rpx;
}
.floor .floor-imgs navigator:nth-child(3),.floor .floor-imgs navigator:nth-child(5){
  width: 223rpx;
  margin-bottom: 10rpx;
}